{
  "name": "dl-vampire",
  "version": "0.5.0",
  "description": "download file like a vampire",
  "main": "index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "index.js",
    "types/index.d.ts",
    "package.json",
    "README.md",
    "LICENSE",
    "vampire.jpeg"
  ],
  "scripts": {
    "test": "mocha && tsd",
    "test-cover": "nyc --reporter=lcov --reporter=text mocha",
    "report-cover": "codecov",
    "prepare": "husky install",
    "build": "rm -rf lib; rm tsconfig.tsbuildinfo; tsc",
    "prepublishOnly": "npm run build",
    "start": "ts-node ./src/index.ts",
    "start:debug": "node --inspect-brk -r ts-node/register ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/dl-vampire.git"
  },
  "keywords": [
    "dl",
    "download",
    "fs",
    "vampire",
    "download-manager"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/dl-vampire/issues"
  },
  "homepage": "https://github.com/magicdawn/dl-vampire#readme",
  "dependencies": {
    "debug": "^4.3.4",
    "fs-extra": "^10.1.0",
    "got": "11",
    "lodash": "^4.17.21",
    "promise.retry": "^0.3.1",
    "proxy-agent": "^5.0.0"
  },
  "devDependencies": {
    "@magicdawn/eslint-config": "^0.0.6",
    "@magicdawn/prettier-config": "^0.0.2",
    "@swc/core": "^1.2.207",
    "@swc/helpers": "^0.4.3",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "codecov": "^3.8.3",
    "eslint": "^8.18.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "should": "^13.2.3",
    "ts-node": "^10.8.1",
    "tsd": "^0.21.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,md}": [
      "prettier --write"
    ]
  }
}
