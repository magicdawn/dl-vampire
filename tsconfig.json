{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "strictNullChecks": true,
    "declaration": true,
    "skipLibCheck": true,
		"types": ["vitest/globals"]
  },
  "ts-node": {
    "swc": true
  }
}
